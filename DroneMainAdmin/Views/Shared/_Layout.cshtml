<body style="overflow: visible;">
    <!--logo Display-->
    <div id="preloader" style="display: none;">
        <div id="status" class="text-center" style="display: none;">
            <img src="/images/promaxLogo.png" alt="ProMax">
        </div>
    </div>
    <header id="header">
        <div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar" aria-expanded="false">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    @*<a>
                            @Html.ActionLink("DFest", "Login", "User", new { area = "" }, new { @class = "navbar-brand" })
                        </a>*@
                </div>
                <div id="navbar" class="collapse navbar-collapse">

                    <ul class="nav navbar-nav">
                        <li class="active">@Html.ActionLink("DFest", "Login", "User")</li>
                        <li>@Html.ActionLink("Login", "Login", "User")</li>
                        <li>@Html.ActionLink("Sign Up", "Registration", "User")</li>
                        <li>@Html.ActionLink("Team List", "Index", "TeamList")</li>
                    </ul>
                    <ul class="nav navbar-nav navbar-right">

                        <li><a> welcome @HttpContext.Current.User.Identity.Name</a></li>
                        <li>
                            @if (Request.IsAuthenticated)
                            {
                                using (Html.BeginForm("Logout", "User", FormMethod.Post, new { id = "logoutForm" }))
                                {
                                    <div class="btn btn-pink" style="margin-bottom: 7px;margin-top: 7px;">
                                        <p onclick="javascript:document.getElementById('logoutForm').submit()"><i class="fa fa-sign-out" aria-hidden="true"></i> Logout</p>
                                    </div>
                                }
                            }
                        </li>
                    </ul>

                </div>
            </div>
        </div>
    </header>
    <div class="col-md-12 col-sm-12 col-lg-12">
        @RenderBody()
    </div>
       <footer id="footer" class="foot-fixed-bottom">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="footer-social">
                    <a href="https://www.facebook.com/promax.hd.7" target="_blank">
                        <i class="fa fa-facebook"></i>
                        </a>
                       <a href="https://twitter.com/promaxhdevents" target="_blank">
                        <i class="fa fa-twitter"></i>
                       </a>
                        <a href="https://www.linkedin.com/in/sameer-mardikar-06a5aa9?authType=NAME_SEARCH&amp;authToken=K3Fa&amp;locale=en_US&amp;trk=tyah&amp;trkInfo=clickedVertical%3Amynetwork%2CclickedEntityId%3A28342738%2CauthType%3ANAME_SEARCH%2Cidx%3A1-1-1%2CtarId%3A1464415831167%2Ctas%3Asameer" target="_blank">
                        <i class="fa fa-linkedin"></i>
                        </a></div>
                    <div class="footer-text">
                        <p> <a href="#" target="_blank" title="DFest">Powerd By - ProMax</a></p>
                    </div>
                </div>
            </div>
        </div>
    </footer>
